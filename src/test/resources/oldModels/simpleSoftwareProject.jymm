<?xml version="1.0" encoding="UTF-8"?>
<metaModel>
  <name>Simple Software Project</name>
  <classes>
    <class>
      <name>Activity</name>
      <levels>
        <finiteLevel>
          <name>TimeToConclude</name>
          <initialValue>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:ci>duration</m:ci>
            </m:math>
          </initialValue>
        </finiteLevel>
      </levels>
      <infiniteLevels />
      <rates>
        <rate>
          <name>Work</name>
          <expression>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
            <m:apply>
              <m:inverse />
              <m:ci>Prod</m:ci>
            </m:apply>
            </m:math>
          </expression>
          <to>TimeToConclude</to>
        </rate>
      </rates>
      <auxiliaries>
        <property>
          <name>duration</name>
          <value>20.0</value>
        </property>
        <auxiliary>
          <name>Prod</name>
          <expression>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:apply>
                <m:groupsum />
                <m:ci>Team</m:ci>
                <m:ci>Productivity</m:ci>
              </m:apply>
            </m:math>
          </expression>
        </auxiliary>
      </auxiliaries>
      <informations />
    </class>
    <class>
      <name>Developer</name>
      <levels />
      <infiniteLevels />
      <rates />
      <auxiliaries>
        <property>
          <name>experience</name>
          <value>1.0</value>
        </property>
        <auxiliary>
          <name>Productivity</name>
          <expression>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:ci>experience</m:ci>
            </m:math>
          </expression>
        </auxiliary>
      </auxiliaries>
      <informations />
    </class>
  </classes>
  <relations>
    <multiRelation>
      <name>Team</name>
      <source>Activity</source>
      <target>Developer</target>
    </multiRelation>
  </relations>
</metaModel>

