<?xml version="1.0" encoding="UTF-8"?>
<metaModel>
  <name>Simple Software Project</name>
  <classes>
    <class>
      <name>Activity</name>
      <items>
        <finiteStock>
          <name>TimeToConclude</name>
          <initialValue>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:ci>duration</m:ci>
            </m:math>
          </initialValue>
        </finiteStock>
        <property>
          <name>duration</name>
          <value>20.0</value>
        </property>
        <rate>
          <name>Work</name>
          <expression>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:ci>Prod</m:ci>
            </m:math>
          </expression>
          <target>TimeToConclude</target>
        </rate>
        <auxiliary>
          <name>Prod</name>
          <expression>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:apply>
                <m:groupsum />
                <m:ci>Team</m:ci>
                <m:ci>Productivity</m:ci>
              </m:apply>
            </m:math>
          </expression>
        </auxiliary>
      </items>
    </class>
    <class>
      <name>Developer</name>
      <items>
        <property>
          <name>experience</name>
          <value>1.0</value>
        </property>
        <auxiliary>
          <name>Productivity</name>
          <expression>
            <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
              <m:ci>experience</m:ci>
            </m:math>
          </expression>
        </auxiliary>
      </items>
    </class>
  </classes>
  <relations>
    <multiRelation>
      <name>Team</name>
      <source>Activity</source>
      <target>Developer</target>
    </multiRelation>
  </relations>
</metaModel>

