<?xml version="1.0" encoding="UTF-8"?>
<systemDynamicsModel>
  <levels>
    <finiteLevel>
      <name>Population</name>
      <initialValues>1.0</initialValues>
      <ninitialValue>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:cn>1.0</m:cn>
        </m:math>
      </ninitialValue>
    </finiteLevel>
  </levels>
  <infiniteLevels>
    <infiniteLevel>
      <name>PopulatonSource</name>
    </infiniteLevel>
  </infiniteLevels>
  <rates>
    <rate>
      <name>GrowthRate</name>
      <expression>GrowthFactor*Population</expression>
      <nexpression>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:apply>
            <m:times />
            <m:ci>GrowthFactor</m:ci>
            <m:ci>Population</m:ci>
          </m:apply>
        </m:math>
      </nexpression>
      <from>PopulatonSource</from>
      <to>Population</to>
    </rate>
  </rates>
  <auxiliaries>
    <auxiliary>
      <name>GrowthFactor</name>
      <expression>0.5</expression>
      <nexpression>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:cn>0.5</m:cn>
        </m:math>
      </nexpression>
    </auxiliary>
  </auxiliaries>
  <informations>
    <information>
      <name>CurrentFactor</name>
      <source>GrowthFactor</source>
      <consumer>GrowthRate</consumer>
    </information>
    <information>
      <name>CurrentPopulation</name>
      <source>Population</source>
      <consumer>GrowthRate</consumer>
    </information>
  </informations>
</systemDynamicsModel>

