<?xml version="1.0" encoding="UTF-8"?>
<systemDynamicsModel>
  <name>Basic elements of Stock and Flux Diagrams</name>
  <stocks>
    <finiteStock>
      <name>Finite Stock</name>
      <initialValue>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:cn>1.0</m:cn>
        </m:math>
      </initialValue>
    </finiteStock>
  </stocks>
  <infiniteStocks>
    <infiniteStock>
      <name>Infinite Stock</name>
    </infiniteStock>
  </infiniteStocks>
  <rates>
    <rate>
      <name>Rate</name>
      <expression>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:apply>
            <m:times />
            <m:ci>Auxiliary</m:ci>
            <m:ci>Finite Stock</m:ci>
          </m:apply>
        </m:math>
      </expression>
      <source>Infinite Stock</source>
      <target>Finite Stock</target>
    </rate>
  </rates>
  <auxiliaries>
    <auxiliary>
      <name>Auxiliary</name>
      <expression>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:cn>0.5</m:cn>
        </m:math>
      </expression>
    </auxiliary>
  </auxiliaries>
  <informations>
    <information>
      <name>Current Auxiliary Value</name>
      <source>Auxiliary</source>
      <consumer>Rate</consumer>
    </information>
    <information>
      <name>Current Stock Value</name>
      <source>Finite Stock</source>
      <consumer>Rate</consumer>
    </information>
  </informations>
</systemDynamicsModel>

