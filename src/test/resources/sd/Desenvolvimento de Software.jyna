<systemDynamicsModel>
	<name>Desevolvimento de Software</name>
	<stocks>
		<finiteStock>
			<name>Software Desenvolvido</name>
			<initialValue>
				<m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
					<m:cn>0.0</m:cn>
				</m:math>
			</initialValue>
		</finiteStock>
		<finiteStock>
			<name>Requisitos de Software</name>
			<initialValue>
				<m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
					<m:cn>150.0</m:cn>
				</m:math>
			</initialValue>
		</finiteStock>
	</stocks>
	<infiniteStocks>
	</infiniteStocks>
	<rates>
		<rate>
			<name>Desenvolvimento</name>
			<expression>
				<m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
					<m:apply>
						<m:min />
						<m:ci>Requisitos de Software</m:ci>
						<m:ci>Produtividade</m:ci>
					</m:apply>
				</m:math>
			</expression>
			<source>Requisitos de Software</source>
			<target>Software Desenvolvido</target>
		</rate>
	</rates>
	<auxiliaries>
		<auxiliary>
			<name>Produtividade</name>
			<expression>
				<m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
					<m:apply>
						<m:times />
						<m:cn>0.1</m:cn>
						<m:ci>Pessoal</m:ci>
						</m:apply>
				</m:math>
			</expression>
		</auxiliary>
		<auxiliary>
			<name>Pessoal</name>
			<expression>
				<m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
					<m:cn>20</m:cn>
				</m:math>
			</expression>
		</auxiliary>
	</auxiliaries>
	<informations>
		<information>
			<name>Valor de Requisitos de Software</name>
			<source>Requisitos de Software</source>
			<consumer>Desenvolvimento</consumer>
		</information>
		<information>
			<name>Valor de Produtividade</name>
			<source>Produtividade</source>
			<consumer>Desenvolvimento</consumer>
		</information>
		<information>
			<name>Valor de Pessoal</name>
			<source>Pessoal</source>
			<consumer>Produtividade</consumer>
		</information>
	</informations>
</systemDynamicsModel>

