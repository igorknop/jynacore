<systemDynamicsModel>
  <name>Elementos BÃ¡sicos de Estoque e Fluxo</name>
  <stocks>
    <finiteStock>
      <name>Estoque Finito</name>
      <initialValue>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:cn>1.0</m:cn>
        </m:math>
      </initialValue>
    </finiteStock>
  </stocks>
  <infiniteStocks>
    <infiniteStock>
      <name>Estoque Infinito</name>
    </infiniteStock>
  </infiniteStocks>
  <rates>
    <rate>
      <name>Taxa</name>
      <expression>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:apply>
            <m:times />
            <m:ci>Auxiliar</m:ci>
            <m:ci>Estoque Finito</m:ci>
          </m:apply>
        </m:math>
      </expression>
      <source>Estoque Infinito</source>
      <target>Estoque Finito</target>
    </rate>
  </rates>
  <auxiliaries>
    <auxiliary>
      <name>Auxiliar</name>
      <expression>
        <m:math xmlns:m="http://www.w3.org/1998/Math/MathML">
          <m:cn>0.5</m:cn>
        </m:math>
      </expression>
    </auxiliary>
  </auxiliaries>
  <informations>
    <information>
      <name>Valor atual do Auxiliar</name>
      <source>Auxiliar</source>
      <consumer>Taxa</consumer>
    </information>
    <information>
      <name>Valor atual do Estoque Finito</name>
      <source>Estoque Finito</source>
      <consumer>Taxa</consumer>
    </information>
  </informations>
</systemDynamicsModel>

